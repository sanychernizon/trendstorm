<div class="panel-box">
    <div class="select-panel">
        <label class="select-panel-btn" for="{{this._id}}">
            <input type="checkbox" name="panel" id="{{this._id}}" value="{{this._id}}">
        </label>
    </div>
    <div class="panel-info">
        <ul>
            <li>{{ this.name }}</li>
            <li>{{ this.keyword }}</li>
        </ul>
    </div>
    <div class="panel-btns">
        <a class="read-panel-btn" href="/read/{{this._id}}">Ver painel</a>
        <a class="delete-panel-btn" href="/delete/{{this._id}}"><svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1" stroke-linecap="butt" stroke-linejoin="bevel"><polyline points="3 6 5 6 21 6"></polyline><path d="M19 6v14a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2V6m3 0V4a2 2 0 0 1 2-2h4a2 2 0 0 1 2 2v2"></path><line x1="10" y1="11" x2="10" y2="17"></line><line x1="14" y1="11" x2="14" y2="17"></line></svg></a>
    </div>
</div>